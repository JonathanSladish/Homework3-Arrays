    }

    public static ArrayList<Integer> toPower(int size, int power) {
        int index = 0;

        ArrayList exponentialArray;
        for(exponentialArray = new ArrayList(); index < size; ++index) {
            exponentialArray.add((int)Math.pow((double)index, (double)power));
        }

        return exponentialArray;
    }
